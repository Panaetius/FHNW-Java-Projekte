<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:p1="urn:import:com.google.gwt.user.cellview.client">
	<g:HTMLPanel width="1000px" height="750px">
		<g:DockLayoutPanel unit="PX" width="100%" height="100%">
			<g:north size="81.0">
				<g:DockLayoutPanel unit="PX" width="100%" height="40 px">
					<g:south size="1.0">
						<g:Label text="New Label" width="100%" height="1 px" styleName="blackLine"/>
					</g:south>
					<g:west size="80.0">
						<g:Image url="img/Karteikarten.jpg" width="100%" height="100%"/>
					</g:west>
					<g:east size="80.0">
						<g:Image url="img/back.gif" width="100%" height="100%"/>
					</g:east>
					<g:center>
						<g:Label text="Willkommen" styleName="h1" horizontalAlignment="ALIGN_CENTER"/>
					</g:center>
				</g:DockLayoutPanel>
			</g:north>
			<g:center>
				<g:ScrollPanel>
					<p1:CellList ui:field="cellList" width="100%" height="100%"/>
				</g:ScrollPanel>
			</g:center>
			<g:south size="37.0">
				<g:HorizontalPanel height="2em" width="100%" horizontalAlignment="ALIGN_CENTER" verticalAlignment="ALIGN_TOP" borderWidth="0">
					<g:Button text="Lernen" ui:field="learnButton"/>
					<g:Button ui:field="changeButton" text="Aendern"/>
					<g:Button ui:field="newButton" text="Neu"/>
				</g:HorizontalPanel>
			</g:south>
			<g:south size="1.0">
				<g:Label text="New Label" styleName="blackLine"/>
			</g:south>
		</g:DockLayoutPanel>
	</g:HTMLPanel>
</ui:UiBinder> 